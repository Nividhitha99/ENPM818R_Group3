{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": {
          "Federated": "arn:aws:iam::385046010615:oidc-provider/<OIDC_PROVIDER>"
        },
        "Action": "sts:AssumeRoleWithWebIdentity",
        "Condition": {
          "StringEquals": {
            "<OIDC_PROVIDER>:sub": "system:serviceaccount:default:analytics-sa"
          }
        }
      }
    ]
  }
  